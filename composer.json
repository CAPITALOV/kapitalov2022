{
    "name": "dram1008/capitalov",
    "description": "capitalov.com",
    "keywords": ["capitalov"],
    "homepage": "http://www.capitalov.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/dram1008/capitalov"
    },
    "authors": [
        {
            "name": "Dmitriy Mukhortov",
            "email": "dram1008@yandex.ru",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "cs\\": "app"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "nnnick/Chart.js",
                "version": "1.0.1",
                "source": {
                    "url": "https://github.com/nnnick/Chart.js",
                    "type": "git",
                    "reference": "02f858b264de6c6c1af0579b05b7b5913d0d61ef"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "isocra/TableDnD",
                "version": "1.0.1",
                "source": {
                    "url": "https://github.com/isocra/TableDnD",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-authclient": "*",
        "yiisoft/yii2-jui": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yandex-money/yandex-money-sdk-php": "3.0.*",
        "mustangostang/spyc": "*",
        "nnnick/Chart.js": "1.0.1",
        "isocra/TableDnD": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "fxp/composer-asset-plugin": "~1.0",
        "yiisoft/yii2-apidoc": "~2.0.0"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    }
}
